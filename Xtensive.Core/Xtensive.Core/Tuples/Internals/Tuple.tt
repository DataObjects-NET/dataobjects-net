<# for (int j = 0; j <= i; j++) { #>
        typeof (T<#= j #>)<#= j==i ? "" : "," #>
<# } #>
      });
    }


    // Constructors

    /// <inheritdoc/>
    public Tuple(TupleDescriptor descriptor)

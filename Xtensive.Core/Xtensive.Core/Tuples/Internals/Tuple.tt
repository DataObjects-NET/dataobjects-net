    public static void SetValue<#= j #>(Tuple tuple, T<#= j #> value)
    {
      var t = (Tuple<#= GetGenericArgumentsString(i + 1) #>)tuple;
      const long mask = 3L << (<#= j #> << 1);
      if (value == null) 

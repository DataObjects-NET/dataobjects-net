    private const int count = <#= i+1 #>;
	
<# for (int j = 0; j <= i; j++) { #>
    /// <summary>
    /// Value number <#= j #>.
    /// </summary>
    [DataMember]
    public T<#= j #> Value<#= j #>;
	
<# } #>
